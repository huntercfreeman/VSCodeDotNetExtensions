{
	"name": "dot-net-ide",
	"displayName": ".NET Ide",
	"publisher": "HunterFreeman",
	"author": "Hunter Freeman",
	"description": "Add common IDE (Integrated Development environment features to Visual Studio Code for the .NET environment.",
	"version": "1.3.2",
	"engines": {
		"vscode": "^1.68.0"
	},
	"repository": "https://github.com/huntercfreeman/dot-net-ide",
	"icon": "dotnetideicon.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:dot-net-ide.open-xml-editor",
		"onView:dot-net-ide.solution-explorer-webview",
		"onView:dot-net-ide.nuget-package-manager-webview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dot-net-ide",
					"title": ".NET IDE",
					"icon": "media/solutionExplorerIcon.svg"
				}
			]
		},
		"views": {
			"dot-net-ide": [
				{
					"type": "webview",
					"id": "dot-net-ide.solution-explorer-webview",
					"name": "Solution Explorer",
					"icon": "media/solutionExplorerIcon.svg"
				},
				{
					"type": "webview",
					"id": "dot-net-ide.nuget-package-manager-webview",
					"name": "Nuget Package Manager",
					"icon": "media/nugetPackageManagerIcon.svg"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-typescript": "^8.1.0",
		"@tsconfig/svelte": "^1.0.10",
		"@types/polka": "^0.5.1",
		"concurrently": "^5.3.0",
		"rollup": "^2.3.4",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.31.0",
		"svelte-check": "^1.1.23",
		"svelte-preprocess": "^4.6.1",
		"ts-loader": "^8.0.11",
		"typescript": "^4.7.2",
		"@types/vscode": "^1.68.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"eslint": "^8.16.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"vscode-test": "^1.5.0",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-livereload": "^2.0.0",
		"@vscode/test-electron": "^2.1.3"
	},
	"dependencies": {
		"@rollup/plugin-typescript": "^8.3.2",
		"sirv-cli": "^2.0.0",
		"uuidv4": "^6.2.7"
	}
}
